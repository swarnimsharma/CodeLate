
<div class="modal fade" id="import_file_modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Import File</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="divLoading">
                </div>
                @* Hidden *@
                <input type="hidden" id="candidate_pk_id" />
                <div class="row">
                    <form class="form-horizontal form-material">
                        <div class="form-group col-md-12  col-xs-12">
                            <label class="col-md-12 col-xs-12">Upload Type</label>
                            <div class="col-md-12 col-xs-12">
                                <select id="upload_file_type" class="form-control" onchange="">
                                    <option value="">Select Type</option>
                                    <option value="post_requirement">Post Requirement</option>
                                    <option value="candidate">Candidate</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group col-md-12  col-xs-12">
                            <label class="col-md-12 col-xs-12">Upload File</label>
                            <div class="col-md-12 col-xs-12">
                                <input type="file" id="import_upload_file" class="form-control form-control-line">
                                <input type="hidden" id="hidden_import_file_name" />
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="Admin.UploadVendorCandidateFile()">Upload</button>
            </div>
        </div>
    </div>
</div>
